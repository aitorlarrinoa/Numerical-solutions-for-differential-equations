Cell[CellGroupData[{

Cell["\<\
c) Write a program in Mathematica that shows the evolution of the populations \
of foxes and rabbits, correspondent to A = 0.5, B = 0.002, C = 0.1, D = 0.001 \
and initial population of 250 rabbits and 100 foxes.\
\>", "Subsection",
 CellChangeTimes->{{3.821334285763073*^9, 
  3.821334378047675*^9}},ExpressionUUID->"8b123c1f-16d0-4657-9bc1-\
c7e52cb113cc"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"sol", "=", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"x", "'"}], "[", "t", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{"0.5", "*", 
          RowBox[{"x", "[", "t", "]"}]}], "-", 
         RowBox[{"0.002", "*", 
          RowBox[{"x", "[", "t", "]"}], "*", 
          RowBox[{"y", "[", "t", "]"}]}]}]}], ",", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"y", "'"}], "[", "t", "]"}], "\[Equal]", 
        RowBox[{
         RowBox[{
          RowBox[{"-", "0.1"}], "*", 
          RowBox[{"y", "[", "t", "]"}]}], "+", 
         RowBox[{"0.001", "*", 
          RowBox[{"x", "[", "t", "]"}], "*", 
          RowBox[{"y", "[", "t", "]"}]}]}]}], ",", " ", 
       RowBox[{
        RowBox[{"x", "[", "0", "]"}], "\[Equal]", "250"}], ",", 
       RowBox[{
        RowBox[{"y", "[", "0", "]"}], "\[Equal]", "100"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "y"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "300"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Evaluate", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"x", "[", "t", "]"}], "}"}], "/.", "sol"}], "]"}], ",", 
     RowBox[{"Evaluate", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"y", "[", "t", "]"}], "}"}], "/.", "sol"}], "]"}]}], "}"}], 
   ",", 
   RowBox[{"{", 
    RowBox[{"t", ",", "0", ",", "300"}], "}"}], ",", 
   RowBox[{"PlotLegends", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<Rabbits\>\"", ",", "\"\<Foxes\>\""}], "}"}]}]}], 
  "]"}]}], "Input",
 CellChangeTimes->{{3.8213344078986464`*^9, 3.8213344197228394`*^9}, {
