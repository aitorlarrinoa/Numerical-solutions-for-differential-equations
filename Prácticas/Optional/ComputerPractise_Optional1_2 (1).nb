Cell[BoxData[{
 RowBox[{
  RowBox[{"psi3", "[", "x_", "]"}], ":=", " ", "0"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fi3", "[", "x_", "]"}], ":=", 
  RowBox[{"1", "/", 
   RowBox[{"(", 
    RowBox[{"1", " ", "+", " ", 
     RowBox[{"x", "^", "2"}]}], ")"}]}]}]}], "Input",
