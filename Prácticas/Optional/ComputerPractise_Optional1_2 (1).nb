   RowBox[{
    RowBox[{"1", "/", "2"}], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"fi3", "[", 
       RowBox[{"x", "+", " ", 
        RowBox[{"I", " ", "y"}]}], "]"}], " ", "+", " ", 
      RowBox[{"fi3", "[", 
       RowBox[{"x", "-", " ", 
        RowBox[{"I", " ", "y"}]}], "]"}]}], " ", ")"}]}], " ", "-", " ", 
   RowBox[{
    RowBox[{"I", "/", "2"}], " ", 
    RowBox[{"Integrate", "[", 
