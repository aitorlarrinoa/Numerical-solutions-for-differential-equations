      RowBox[{"psi3", "[", "s", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"s", ",", 
        RowBox[{"x", "-", " ", 
         RowBox[{"I", " ", "y"}]}], ",", " ", 
        RowBox[{"x", "+", " ", 
         RowBox[{"I", " ", "y"}]}]}], "}"}]}], "]"}]}]}]}]], "Input",
