Cell[BoxData[{
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"L", " ", "=", " ", "4"}], ";", " ", 
   RowBox[{"inf", " ", "=", " ", "0"}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"per", " ", "=", " ", 
   RowBox[{"L", "-", "inf"}]}], ";", 
  RowBox[{"sup", " ", "=", " ", 
   RowBox[{"inf", " ", "+", " ", "per"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"fact", " ", "=", " ", 
   RowBox[{"2", " ", 
    RowBox[{"Pi", "/", " ", "per"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", " ", 
   RowBox[{"Piecewise", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"2", ",", " ", 
        RowBox[{"0", "\[LessEqual]", " ", "x", "\[LessEqual]", " ", "0"}]}], 
       "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"3", ",", " ", 
        RowBox[{"0", "<", "x", "<", " ", "2"}]}], "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"2", ",", 
        RowBox[{"2", "\[LessEqual]", "  ", "x", "\[LessEqual]", "  ", "2"}]}],
        "}"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"1", ",", 
        RowBox[{"2", "<", "x", "\[LessEqual]", " ", "4"}]}], "}"}]}], "}"}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"f", "[", "x", "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", " ", "inf", ",", "L"}], "}"}], ",", 
   RowBox[{"PlotRange", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"0", ",", "3"}], "}"}]}]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"a", "[", "n_", "]"}], ":=", 
   RowBox[{
    RowBox[{"a", "[", "n", "]"}], "=", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"2", "/", "per"}], ")"}], " ", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"f", "[", "t", "]"}], " ", 
        RowBox[{"Cos", "[", 
         RowBox[{"fact", " ", "n", " ", "t"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "inf", ",", "sup"}], "}"}]}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"b", "[", "n_", "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"b", "[", "n", "]"}], "=", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"2", "/", "per"}], ")"}], " ", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"f", "[", "t", "]"}], " ", 
        RowBox[{"Sin", "[", 
         RowBox[{"fact", " ", "n", " ", "t"}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "inf", ",", "sup"}], "}"}]}], "]"}]}]}]}], " ", 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"A", "[", "k_", "]"}], ":=", 
   RowBox[{
    RowBox[{"A", "[", "k", "]"}], "=", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"k", " ", ">", " ", "0"}], ",", " ", 
      RowBox[{"a", "[", "k", "]"}], ",", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", "2"}], ")"}], 
       RowBox[{"a", "[", "0", "]"}]}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"B", "[", "k_", "]"}], ":=", 
   RowBox[{
    RowBox[{"B", "[", "k", "]"}], "=", 
    RowBox[{"b", "[", "k", "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"F", "[", 
    RowBox[{"x_", ",", "n_"}], "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"F", "[", 
     RowBox[{"x", ",", "n"}], "]"}], "=", 
    RowBox[{"Simplify", "[", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"A", "[", "k", "]"}], " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"fact", " ", "k", " ", "x"}], "]"}]}], "+", 
        RowBox[{
         RowBox[{"B", "[", "k", "]"}], " ", 
         RowBox[{"Sin", "[", 
          RowBox[{"fact", " ", "k", " ", "x"}], "]"}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "0", ",", "n"}], "}"}]}], "]"}], "]"}]}]}], 
  ";"}]}], "Input",
