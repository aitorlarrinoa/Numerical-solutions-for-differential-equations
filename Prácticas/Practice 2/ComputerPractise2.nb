Cell[BoxData[{
 RowBox[{
  RowBox[{"L3", " ", "=", " ", "1"}], ";", 
  RowBox[{"c3", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"inf3", "=", " ", "0"}], ";", " ", 
  RowBox[{"sup3", " ", "=", 
   RowBox[{"inf3", " ", "+", " ", "L3"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"fact3", "=", 
    RowBox[{"Pi", "/", " ", "L3"}]}], ";"}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"NTERM3", "=", "5"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f3", "[", "x_", "]"}], ":=", 
   RowBox[{"Sin", "[", 
    RowBox[{"Pi", "*", "x"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
